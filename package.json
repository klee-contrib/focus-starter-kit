{
  "author": "focus@kleegroup.com",
  "name": "focus-starter-kit",
  "description": "front-end demo spa",
  "version": "3.0.0",
  "homepage": "https://github.com/KleeGroup/focus-starter-kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/KleeGroup/focus-starter-kit.git"
  },
  "engines": {
    "node": "0.4.x",
    "npm": "2.14.x"
  },
  "scripts": {
    "bundle": "better-npm-run bundle",
    "build": "npm run bundle",
    "start": "better-npm-run webpack-dev-server",
    "watch": "better-npm-run bundle-watch",
    "webpack-dev-server": "better-npm-run webpack-dev-server",
    "profile": "better-npm-run profile"
  },
  "betterScripts": {
    "bundle": {
      "command": "webpack --progress",
      "env": {
        "ANCHOR_CLASS": "your-app",
        "ENTRY_FILE_PATH": "./app/index",
        "BABELIFIED_PATH": "./app",
        "DEV": false,
        "NODE_ENV": "production",
        "LIBRARY_NAME": "YourApp",
        "OUTPUT_DIR": "./public",
        "PACKAGE_JSON_PATH": "./"
      }
    },
    "bundle-watch": {
      "command": "webpack --progress --watch",
      "env": {
        "ANCHOR_CLASS": "your-app",
        "ENTRY_FILE_PATH": "./app/index",
        "BABELIFIED_PATH": "./app",
        "DEV": false,
        "LIBRARY_NAME": "YourApp",
        "OUTPUT_DIR": "./public",
        "PACKAGE_JSON_PATH": "./"
      }
    },
    "webpack-dev-server": {
      "command": "node ./dev-server.js",
      "env": {
        "ANCHOR_CLASS": "your-app",
        "ENTRY_FILE_PATH": "./app/index",
        "BABELIFIED_PATH": "./app",
        "PAGE_TITLE": "Your application",
        "API_PORT": 8080,
        "OUTPUT_DIR": "./public",
        "PACKAGE_JSON_PATH": "./"
      }
    }
  },
  "babel": {
    "presets": [
      "stage-0",
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy",
      "add-module-exports"
    ]
  },
  "main": "app/index.js",
  "license": "MIT",
  "dependencies": {
    "focus-components": "0.8.6-beta4",
    "focus-core": "0.12.4-beta4",
    "jquery": "^2.2.0",
    "lodash": "^4.0.1",
    "moment": "^2.10.3",
    "numeral": "^1.5.3",
    "react": "^0.14.1",
    "react-dom": "^0.14.1"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.0.5",
    "css-loader": "^0.21.0",
    "eslint": "1.5.1",
    "eslint-config-focus": "0.3.0",
    "eslint-plugin-filenames": "0.1.1",
    "eslint-plugin-react": "3.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.3",
    "node-sass": "^3.4.2",
    "react-hot-loader": "1.3.0",
    "sass-loader": "^3.0.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.12",
    "webpack-dev-server": "1.14.1",
    "webpack-focus": "^0.9.1"
  }
}
